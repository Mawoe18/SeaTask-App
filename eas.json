{
  "cli": {
    "version": ">= 13.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "20.18.0",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "20.18.0",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "LIGHTNINGCSS_PLATFORM": "linux-x64-gnu",
        "npm_config_target_platform": "linux",
        "npm_config_target_arch": "x64"
      }
    },
    "production": {
      "node": "20.18.0",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "production",
        "LIGHTNINGCSS_PLATFORM": "linux-x64-gnu",
        "npm_config_target_platform": "linux",
        "npm_config_target_arch": "x64"
      }
    }
  },
  "submit": {
    "production": {}
  }
}